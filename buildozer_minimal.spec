[build]
# 日誌等級
log_level = 2

# 本地版本
local_version_path = .

# 編譯的應用檔案名
appname = JeffMingliApp

[app]
# 應用標題
title = Jeff命理世界

# 套件名稱
package.name = jiuyin_destiny

# 套件域名
package.domain = org.jiuyin

# 原始檔位置
source.dir = .

# 版本號
version = 6.7

# 需求（Python 依賴）
requirements = python3,kivy,pillow

# 方向
orientation = portrait

# 全螢幕
fullscreen = 0

[buildozer]
# 日誌等級
log_level = 2

# 警告等級
warn_on_root = 1
